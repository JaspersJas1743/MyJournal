{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Health for databases",
        "Uri": "/health/db"
      },
      {
        "Name": "Health for db context",
        "Uri": "/health/context"
      },
      {
        "Name": "Health for Redis",
        "Uri": "/health/redis"
      },
      {
        "Name": "Health for AWS",
        "Uri": "/health/aws"
      },
      {
        "Name": "Health for S3 Storage",
        "Uri": "/health/s3"
      },
      {
        "Name": "Health for SignalR",
        "Uri": "/health/signalR"
      }
    ],
    "EvaluationTimeInSeconds": 10
  },
  "AllowedHosts": "*"
}
